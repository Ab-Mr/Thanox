    <string name="feature_category_apps">應用</string>
    <string name="feature_title_smart_app_freeze">智能凍結</string>
    <string name="menu_title_smart_app_freeze_enable_all_smart_freeze_apps">全部解凍並移除列表中的應用</string>
    <string name="menu_desc_smart_app_freeze_enable_all_smart_freeze_apps">將智能凍結的應用全部解凍，並從智能凍結列表中移除，你確定要這樣做嗎？</string>
    <string name="menu_title_smart_app_freeze_enable_all_apps_smart_freeze_temp">全部臨時解凍列表中的應用</string>
    <string name="menu_desc_smart_app_freeze_enable_all_apps_smart_freeze_temp">將智能凍結列表中的應用全部臨時解凍，你確定要這樣做嗎？</string>
    <string name="menu_title_smart_app_freeze_enable_all_apps">解凍所有應用</string>
    <string name="menu_title_smart_app_freeze_export_package_list">匯出應用列表</string>
    <string name="menu_title_smart_app_freeze_import_package_list">匯入應用列表</string>
    <string name="menu_desc_smart_app_freeze_enable_all_apps">將會解凍你的系統中所有被凍結或掛起的應用，包含所有已安裝或系統應用，你確定要這樣做嗎？</string>
    <string name="feature_summary_smart_app_freeze">把不常用的應用凍結起來</string>
    <string name="feature_title_enable_package_on_launch">活檢解凍（DOL）</string>
    <string name="feature_summary_enable_package_on_launch">應用被智能凍結後，當檢測到該應用的活動啟動請求，Thanox會自動解凍該應用</string>
    <string name="pref_title_enable_package_on_launch_by_default">自動開啟\"活檢解凍\"</string>
    <string name="pref_title_dol_tips">DOL提示</string>
    <string name="pref_summary_enable_package_on_launch_by_default">當某個應用被加入智能凍結列表時，自動為這個應用開啟\"活檢解凍\"開關</string>
    <string name="feature_desc_smart_app_freeze">注意，凍結應用使用了Android系統的能力，意味著這些狀態會跟隨系統持久化，即使你移除了Thanox，這些狀態將依然會保留</string>
    <string name="menu_title_create_shortcut">建立啟動快捷方式</string>
    <string name="menu_title_create_shortcut_apk">產生代理應用</string>
    <string name="menu_title_broadcast_intent_shortcut">廣播意圖</string>
    <string name="menu_title_activity_intent_shortcut">活動意圖</string>
    <string name="dialog_message_create_shortcut_apk">產生一個專門用於啟動該凍結應用的綠色Android app，產生的應用將只包含一個Activity，大約30K。你可以安裝代理應用來解決桌面沒有凍結應用圖示問題。</string>

    <string name="dialog_title_smart_app_standby_need_enable_platform_app_idle">開啟自適應電池</string>
    <string name="dialog_message_smart_app_standby_need_enable_platform_app_idle">
        Android 9+設備使用智能休眠需要開啟系統設置中的電池-自適應電池選項才能正常工作。\n\nThanox目前無法準確判斷你的系統是否支持並開啟了該選項。
    </string>
    <string name="message_do_you_want_to_add_apps_to_pkg_set">還要將這些應用程式添加到目前的應用集中嗎？</string>

